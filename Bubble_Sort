#include<bits/stdc++.h>
using namespace std;

void bubble_sort(vector<int> vec){
    int n=vec.size();
    for(int i=0;i<n-1;i++){
        for(int j=0;j<n-1-i;j++){
            if(vec[j]>vec[j+1]){
                swap(vec[j],vec[j+1]);
            }
        }
    }
    for(int i=0;i<n;i++){
        cout<<vec[i]<<" ";
    }
}
int main(){
    vector<int> vec;
    int size,c;
    cout<<"Size: ";
    cin>> size;
    for(int i=0;i<size;i++){
        cin>>c;
        vec.push_back(c);
    }
    bubble_sort(vec);
}
