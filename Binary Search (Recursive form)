#include<bits/stdc++.h>
using namespace std;

int binarySearch(vector<int> arr , int key, int start, int end){
    while(start<=end){
        int mid= (start + ((end-start)/2));
        if(arr[mid] < key)
            return binarySearch(arr,key,mid+1,end);
        else if(arr[mid] > key)
            return binarySearch(arr,key,start,mid-1);
        else
            return mid;
    }
    return -1;
}

int main(){
    vector<int>arr;
    int n,x,key;
    cout<<"Enter the size of the array:";
    cin>>n;
    cout<<"The numbers are: ";
    for(int i=0;i<n;i++){
        cin>>x;
        arr.push_back(x);
    }
    cout<<"Enter the number we need to find out:";
    cin>>key;
    int start=0;
    int end=arr.size()-1;
    int find= binarySearch(arr, key, start, end);
    if(find == -1)
        cout<<"The required number is not found.";
    else
        cout<<"The Required number is found at position "<< find+1;
}
